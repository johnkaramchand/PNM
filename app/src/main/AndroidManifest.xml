<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.atria.gov.demo">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity
            android:name=".Intro.SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Intro.WelcomeAct"
            android:theme="@style/SplashTheme2" />
        <activity android:name=".BottomNavigationActivity" />
        <activity android:name=".PhoneAuthActivity" />
        <activity android:name=".VerifyPhoneActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".NotificationsActivity" />
        <activity android:name=".ReportActivity" />
        <activity android:name=".PanchayatActivity" />
        <activity android:name=".idk" />
        <activity android:name=".GrivenceActivity" />
        <activity android:name=".IssuesActivity"></activity>
    </application>

</manifest>